CREATE TABLE clients (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(255),
    docType VARCHAR(10),
    docNumber VARCHAR(20) UNIQUE,
    address VARCHAR(255),
    district VARCHAR(255),
    phone VARCHAR(20),
    email VARCHAR(255),
    plan VARCHAR(100),
    crmStatus DATETIME,
    releaseTime DATETIME,
    observations TEXT,
    orderStatus VARCHAR(50)
);
